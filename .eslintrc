{
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 1,
    "strict": 0,
    "no-trailing-spaces": [2],
    "indent": [
      2,
      2,
      {
        "VariableDeclarator": 2,
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      2,
    "unix"
    ],
    "semi": [
      2,
    "always"
    ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true,
    "arrowFunctions": true
  },
  "plugins": [
    "react"
  ]
}
